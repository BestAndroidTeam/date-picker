<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout"
    >
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_1"
      style="@style/WeekRowView.WeekdayHeader.Start"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_2"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="S"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_2"
      style="@style/WeekRowView.WeekdayHeader"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_3"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_1"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="M"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_3"
      style="@style/WeekRowView.WeekdayHeader"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_4"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_2"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="T"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_4"
      style="@style/WeekRowView.WeekdayHeader"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_5"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_3"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="W"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_5"
      style="@style/WeekRowView.WeekdayHeader"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_6"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_4"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="T"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_6"
      style="@style/WeekRowView.WeekdayHeader"
      app:layout_constraintEnd_toStartOf="@+id/weekday_header_7"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_5"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="F"
      />
  <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/weekday_header_7"
      style="@style/WeekRowView.WeekdayHeader.End"
      app:layout_constraintStart_toEndOf="@+id/weekday_header_6"
      app:layout_constraintTop_toBottomOf="@id/current_month"
      tools:text="S"
      />
</merge>
