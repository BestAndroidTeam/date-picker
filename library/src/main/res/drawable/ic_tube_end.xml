<?xml version="1.0" encoding="utf-8"?>
<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="@dimen/week_day_row_disabled_background_inset"
    android:insetTop="@dimen/week_day_row_disabled_background_inset">
  <shape android:shape="rectangle">
    <corners
        android:bottomRightRadius="@dimen/week_day_row_height"
        android:topRightRadius="@dimen/week_day_row_height"/>
    <solid android:color="#000000"/>
    <size
        android:height="@dimen/week_day_row_disabled_background_height"
        android:width="@dimen/week_day_row_disabled_background_height"/>
    <padding
        android:bottom="@dimen/week_day_row_disabled_background_inset"
        android:left="@dimen/week_day_row_disabled_background_inset"
        android:right="@dimen/week_day_row_disabled_background_inset"
        android:top="@dimen/week_day_row_disabled_background_inset"/>
  </shape>
</inset>
